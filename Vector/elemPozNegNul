#include <iostream>

using namespace std;

int main()
{
    int v[100], n, pozitiv = 0, negativ = 0, nul = 0;
    cin >> n;
    for(int i=1; i <= n; i++){
        cin >> v[i];
        if(v[i] > 0)
            pozitiv++;
        else if (v[i] < 0)
            negativ++;
        else
            nul++;
    }
    cout << "Sunt " <<  pozitiv << " numere pozitive. Sunt " << negativ  << " numere negative si "<< nul << " nule!";



    return 0;
}
