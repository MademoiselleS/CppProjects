#include <iostream>

using namespace std;

int main()
{
    float Ma;
    int v[100], n, suma, nr;
    cin >> n;

    for(int i = 1; i <= n; i++)
        cin >> v[i];

    suma = 0;
    nr = 0;
    for(int i = 1; i <= n; i++){
       if(v[i] % 2 == 0 && v[i] > 0){
          suma += v[i];
          nr++;

    }
    }

    Ma = suma / nr;
    cout << Ma;

    return 0;
}
