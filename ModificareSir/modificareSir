#include<iostream>

using namespace std;


int main(){

    int N, v[999], i = 1;
    cin >> N;

    for (i = 1; i <= N; i++)
        cin >> v[i];
    for (i = 1; i <= N; i++)
        if (i % 2 == 0)
            v[i] = v[i] * 2;
        else
            v[i] = v[i] - 1;

    for (i = 1; i <= N; i++)
        cout << v[i] << " ";


    return 0;
}
