#include<iostream>

using namespace std;

int main(){

    int v[100], n;

    cout << " Dati numarul de elemente ale vectorului";
    cin >> n;

//citirea vectorului de la tastatura
    for(int i = 1; i <= n; i++)
        cin >> v[i];

    int sum = 0;
    for(int i = 1; i <= n ; i++){
        if(v[i] > 0)
            sum = sum +  v[i];
    }

    cout << " Suma elementelor pozitive este "  << sum;

    return 0;
}

